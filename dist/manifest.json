{
  "manifest_version": 3,
  "name": "Pocket Quant Trader",
  "version": "0.1.0",
  "description": "Automated trading assistant for Pocket Option with backtesting and analysis",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "sidePanel",
    "alarms"
  ],
  "host_permissions": [
    "https://pocketoption.com/*",
    "https://*.pocketoption.com/*"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/index.ts-Bh4srEP3.js"
      ],
      "matches": [
        "https://pocketoption.com/*",
        "https://*.pocketoption.com/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "side_panel": {
    "default_path": "src/side-panel/index.html"
  },
  "action": {
    "default_title": "Open Pocket Quant Trader"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "https://*.pocketoption.com/*",
        "https://pocketoption.com/*"
      ],
      "resources": [
        "assets/index.ts-Bh4srEP3.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
